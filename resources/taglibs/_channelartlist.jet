{{block channelartlist(typeid=0,row=20)  }}
    {{if !isset(__typeid)}}
        {{__typeid = 0}}
    {{end}}
    {{channellist = channelartlist(typeid, row, __typeid)}}
    {{ range autoindex,field = channellist }}
        {{__refID = field.Catid }}
        {{yield content}}
    {{end}}
{{end}}
