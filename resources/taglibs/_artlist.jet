{{block artlist(typeid = -1,offset=0,limit=10, orderby="listorder",field="", modelid = "")  }}
    {{if typeid == -1 }}
    {{ if isset(__typeid) }}
        {{typeid = __typeid}}
    {{end}}
    {{end}}
    {{ range field = artlist(typeid, offset, limit, orderby, modelid) }}
        {{yield content}}
    {{end}}
{{end}}
